{
  "legislative_session": "2026A",
  "identifier": "HB 1092",
  "title": "Licensed Midwife Public Health Facility Privileges",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Health Care & Health Insurance",
    "Professions & Occupations"
  ],
  "abstracts": [
    {
      "note": "summary",
      "abstract": "The bill prohibits a public health facility from denying, restricting, or conditioning the granting, use, or exercise of full medical staff privileges to a certified nurse midwife or a certified midwife (licensee) engaged in independent practice within their scope of practice solely on the basis of their licensure. If granted, privileges extended to a licensee must include admitting and discharge privileges, be consistent with those of other medical staff, and permit a licensee to exercise the same voting rights as other members of the medical staff.(Note: This summary applies to this bill as introduced.)"
    }
  ],
